{
  "name": "horde",
  "private": true,
  "version": "0.0.1",
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "scripts": {
    "start": "npm run codegen && meteor run",
    "codegen": "npm run codegen:scaphold",
    "codegen:scaphold": "node_modules/.bin/gql-gen --url https://us-west-2.api.scaphold.io/graphql/tame-holiday --template typescript-multiple --out ./types/gen-types/scaphold/ ./client/**/*.ts",
    "tcm": "node_modules/.bin/tcm client",
    "tcm:watch": "node_modules/.bin/tcm -w client"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "apollo-cache-inmemory": "^1.2.9",
    "apollo-client": "^2.4.1",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-server": "^2.0.7",
    "apollo-server-express": "^2.0.6",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-server-express": "^1.4.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "http-proxy-middleware": "^0.19.0",
    "isomorphic-fetch": "^2.2.1",
    "meteor-node-stubs": "^0.4.1",
    "react": "^16.5.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/http-proxy-middleware": "^0.17.5",
    "@types/meteor": "^1.4.20",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "apollo-codegen": "^0.20.2",
    "graphql-code-generator": "^0.11.0",
    "node-sass": "^4.9.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typed-css-modules": "^0.3.5"
  },
  "cssModules": {
    "ignorePaths": [
      "node_modules"
    ]
  }
}
