{
  "name": "horde",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "npm run codegen && meteor run",
    "codegen": "npm run codegen:scaphold",
    "codegen:scaphold": "node_modules/.bin/gql-gen --url https://us-west-2.api.scaphold.io/graphql/tame-holiday --template typescript-multiple --out ./types/gen-types/scaphold/ ./client/**/*.ts",
    "tcm": "node_modules/.bin/tcm client",
    "tcm:watch": "node_modules/.bin/tcm -w client"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.1.0",
    "apollo-link-http": "^1.3.2",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-server-express": "^1.3.2",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.14.1",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "meteor-node-stubs": "^0.3.2",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.0",
    "@types/http-proxy-middleware": "^0.17.2",
    "@types/meteor": "^1.4.12",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "apollo-codegen": "^0.18.3",
    "graphql-code-generator": "^0.8.14",
    "node-sass": "^4.7.2",
    "typed-css-modules": "^0.3.1"
  },
  "cssModules": {
    "ignorePaths": [
      "node_modules"
    ]
  }
}
